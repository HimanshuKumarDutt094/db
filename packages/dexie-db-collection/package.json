  "repository": {
    "type": "git",
    "url": "https://github.com/TanStack/db.git",
    "directory": "packages/dexie-db-collection"
  },
  "homepage": "https://tanstack.com/db",
  "keywords": [
    "dexie",
    "nosql",
    "realtime",
    "local-first",
    "sync",
    "indexeddb",
    "localstorage",
    "optimistic",
    "typescript"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint . --fix",
    "test": "npx vitest --run"
  },
  "sideEffects": false,
  "type": "module",
  "types": "dist/esm/index.d.ts"
